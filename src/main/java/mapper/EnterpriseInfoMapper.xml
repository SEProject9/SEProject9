<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

	<mapper namespace="mapper.EnterpriseInfoMapper">
	    <insert id="add" parameterType="EnterpriseInfo" >
	        insert into enterprise_info_list ( name ) values (#{name})    
	    </insert>
	    
	    <delete id="delete" parameterType="EnterpriseInfo" >
	        delete from enterprise_info_list where id= #{id}   
	    </delete>
	    
	    <select id="get" parameterType="_int" resultType="EnterpriseInfo">
	        select * from   enterprise_info_list  where id= #{id}    
	    </select>

	    <update id="update" parameterType="EnterpriseInfo" >
	        update enterprise_info_list set name=#{name} where id=#{id}    
	    </update>
	    <select id="list" resultType="EnterpriseInfo">
	        select * from   enterprise_info_list      
	    </select>	    
	</mapper>