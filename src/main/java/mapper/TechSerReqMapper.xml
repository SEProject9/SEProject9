<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="mapper.TechSerReqMapper">
	<insert id="add" parameterType="TechSerReq">
		insert into tech_ser_req_list ( name ) values (#{name})
	</insert>

	<delete id="delete" parameterType="TechSerReq">
		delete from tech_ser_req_list where id= #{id}
	</delete>

	<select id="get" parameterType="_int" resultType="TechSerReq">
		select * from tech_ser_req_list where id= #{id}
	</select>

	<update id="update" parameterType="TechSerReq">
		update tech_ser_req_list set name=#{name} where id=#{id}
	</update>
	<select id="list" resultType="TechSerReq">
		select * from tech_ser_req_list
	</select>
</mapper>