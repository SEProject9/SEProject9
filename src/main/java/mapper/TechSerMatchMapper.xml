<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

	<mapper namespace="mapper.TechSerMatchMapper">
	    <insert id="add" parameterType="TechSerMatch" >
	        insert into tech_ser_match_list ( name ) values (#{name})    
	    </insert>
	    
	    <delete id="delete" parameterType="TechSerMatch" >
	        delete from tech_ser_match_list where id= #{id}   
	    </delete>
	    
	    <select id="get" parameterType="_int" resultType="TechSerMatch">
	        select * from   tech_ser_match_list  where id= #{id}    
	    </select>

	    <update id="update" parameterType="TechSerMatch" >
	        update tech_ser_match_list set name=#{name} where id=#{id}    
	    </update>
	    <select id="list" resultType="TechSerMatch">
	        select * from   tech_ser_match_list      
	    </select>	    
	</mapper>