<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="mapper.AdminMapper">
	<insert id="add" parameterType="Admin">
		insert into admin_list ( name ) values (#{name})
	</insert>

	<delete id="delete" parameterType="Admin">
		delete from admin_list where id= #{id}
	</delete>

	<select id="get" parameterType="_int" resultType="Admin">
		select * from admin_list where id= #{id}
	</select>

	<update id="update" parameterType="Admin">
		update admin_list set name=#{name} where id=#{id}
	</update>
	<select id="list" resultType="Admin">
		select * from admin_list
	</select>
</mapper>